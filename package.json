{
  "name": "anime-searcher",
  "version": "2.0.0",
  "description": "Discord Bot to Search an Anime from Image Source. Easily find anime from given Image URL.",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "ts-node --transpile-only ./src/index.ts",
    "dev": "ts-node-dev --respawn --cls --rs --transpile-only --poll ./src/index.ts",
    "build": "tsc",
    "format": "prettier -w .",
    "check-format": "prettier -c ."
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@types/node": "^17.0.7",
    "anilist-node": "^1.11.0",
    "chalk": "^4.1.2",
    "consola": "^2.15.3",
    "discord.js": "^13.5.0",
    "djs-template-cli": "^1.2.1",
    "dotenv": "^10.0.0",
    "glob": "^7.2.0",
    "mongoose": "^5.13.14",
    "node-fetch": "2.6.6"
  },
  "devDependencies": {
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  },
  "keywords": [
    "bot",
    "discord",
    "discord-js",
    "anime",
    "search",
    "anime-search"
  ]
}
