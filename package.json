{
  "name": "anime-searcher",
  "version": "2.0.0",
  "description": "Discord Bot to Search an Anime from Image Source. Easily find anime from given Image URL.",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "ts-node --transpile-only ./src/index.ts",
    "dev": "ts-node-dev --respawn --cls --rs --transpile-only --poll ./src/index.ts",
    "build": "tsc",
    "format": "prettier -w .",
    "check-format": "prettier -c ."
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@types/node": "17.0.23",
    "anilist-node": "1.12.1",
    "chalk": "4.1.2",
    "consola": "2.15.3",
    "discord.js": "13.8.1",
    "djs-template-cli": "1.6.2",
    "dotenv": "16.0.1",
    "glob": "8.0.3",
    "mongoose": "5.13.14",
    "node-fetch": "2.6.7"
  },
  "devDependencies": {
    "prettier": "2.7.1",
    "ts-node": "10.8.1",
    "ts-node-dev": "2.0.0",
    "typescript": "4.7.4"
  },
  "keywords": [
    "bot",
    "discord",
    "discord-js",
    "anime",
    "search",
    "anime-search"
  ]
}
